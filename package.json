{
  "name": "face-recognition-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:https": "node server.js",
    "setup:https": "node scripts/setup-https.js",
    "build": "npm run setup:deploy || true && next build",
    "start": "next start",
    "lint": "eslint",
    "init-db": "npx tsx data/init.ts",
    "inject-data": "npx tsx data/inject-data.ts",
    "check-seed": "npx tsx data/check-seed.ts",
    "create-admin": "npx tsx scripts/create-admin.ts",
    "train-model": "npx tsx scripts/train-skin-model.ts",
    "train-fast": "npx tsx scripts/train-fast.ts",
    "setup:deploy": "npx tsx scripts/setup-deploy-db.ts",
    "postinstall": "node -e \"if (process.env.RUN_SETUP_DEPLOY === '1') { require('child_process').execSync('npx tsx scripts/setup-deploy-db.ts', { stdio: 'inherit', cwd: process.cwd() }); }\""
  },
  "dependencies": {
    "@mediapipe/face_detection": "^0.4.1646425229",
    "@mediapipe/face_mesh": "^0.4.1633559619",
    "@supabase/supabase-js": "^2.89.0",
    "@tensorflow-models/face-landmarks-detection": "^1.0.2",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-backend-webgl": "^4.22.0",
    "@types/pg": "^8.16.0",
    "better-auth": "^1.4.4",
    "dotenv": "^17.2.3",
    "next": "15.5.9",
    "pdfkit": "^0.15.0",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sharp": "^0.33.5",
    "sweetalert2": "^11.26.17",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "typescript": "5.9.3"
  }
}
